@model IEnumerable<Chuyenphatnhanh.Models.BranchMstForm>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutList.cshtml";
} 
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BRANCH_NAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ADDRESS)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LATITUDE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LONGITUDE)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.BRANCH_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Display_Address)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LATITUDE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LONGITUDE)
                </td>
                <td>
                    @Html.ActionLink(Chuyenphatnhanh.Content.Texts.RGlobal.Edit, "Edit", new { id = item.BRANCH_ID }) |
                    @Html.ActionLink(Chuyenphatnhanh.Content.Texts.RGlobal.View, "Details", new { id = item.BRANCH_ID }) |
                    @Html.ActionLink(Chuyenphatnhanh.Content.Texts.RGlobal.Delete, "Delete", new { id = item.BRANCH_ID })
                </td>
            </tr>
        }
    </tbody>    
</table>
